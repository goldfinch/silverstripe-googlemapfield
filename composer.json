{
    "name": "goldfinch/silverstripe-googlemapfield",
    "type": "silverstripe-vendormodule",
    "description": "Save locations using latitude/longitude DataObject fields.",
    "keywords": ["maps","google", "googlemapfield", "silverstripe", "map", "geolocation"],
    "homepage": "http://github.com/goldfinch/googlemapfield",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Will Morgan",
            "homepage": "http://twitter.com/willmorgan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "silverstripe/framework": "^4 || ^5"
    },
    "replace": {
		"betterbrief/silverstripe-googlemapfield": "self.version"
	},
    "autoload": {
        "psr-4": {
            "BetterBrief\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/css",
            "client/js"
        ]
    }
}
